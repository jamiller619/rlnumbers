{
  "name": "rlnumbers",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dx": "yarn clean && yarn copy && NODE_ENV=development node --loader ts-node/esm scripts/watch.ts",
    "test": "jest",
    "build": "yarn clean && yarn copy && node --loader ts-node/esm scripts/build.ts",
    "clean": "rimraf dist/{*.cjs,*.map,*.html,assets}",
    "db": "run-s db:*",
    "db:clean": "rimraf ./src/main/db/rlnumbers.db",
    "db:gen": "yarn prisma generate --schema=./src/main/db/schema.prisma",
    "db:push": "yarn prisma db push --schema=./src/main/db/schema.prisma",
    "copy": "run-p copy:*",
    "copy:rrrocket": "node --loader ts-node/esm scripts/copy.ts ./src/main/lib/rrrocket/bin ./dist/bin",
    "copy:intl": "node --loader ts-node/esm scripts/copy.ts ./src/main/intl/locales ./dist/locales"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^28.1.6",
    "@types/klaw": "^3.0.3",
    "@types/node": "^17.0.21",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/recursive-readdir": "^2.2.1",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-react": "^2.0.0",
    "chalk": "^5.0.1",
    "deepmerge": "^4.2.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prisma": "^3.14.0",
    "prisma-dbml-generator": "^0.9.1",
    "recursive-readdir": "^2.2.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "vite": "^3.0.2",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@radix-ui/colors": "^0.1.8",
    "chokidar": "^3.5.3",
    "dotenv": "^16.0.1",
    "electron": "^18.2.3",
    "electron-store": "^8.1.0",
    "klaw": "^4.0.1",
    "node-jq": "^2.3.3",
    "os-locale": "5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5",
    "styled-reset": "^4.4.2",
    "styled-system": "^5.1.5",
    "swr": "^1.3.0",
    "usehooks-ts": "^2.6.0",
    "winston": "^3.7.2",
    "zustand": "^3.7.2"
  }
}
